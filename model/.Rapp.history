 model<-function(t,state,parameters){#
  with(as.list(c(state,parameters)),{  #
  	f1 <- 1/(1 + b1*X1)#
  	f2 <- 1/(1 + b2*X2)#
  	f3 <- 1/(1 + c2*b3*(X1 + X2))#
  	dX1 <- X1*(1-X1-c1*alpha*X3) - a*X1*(P1*f1 + c2*P3*f3)#
  	dX2 <- X2*(1-X2-c1*alpha*X3) - a*X2*(P2*f2 + c2*P3*f3)#
  	dX3 <- X3*(1-X3-c1*alpha*(X1+X2))#
  	dP1 <- a*P1*f1*X1 - m*P1#
  	dP2 <- a*P2*f2*X2 - m*P2#
